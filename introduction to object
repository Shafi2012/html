    </script>
    <p>Find the largest number</p>
    <p id="large number"></p>
    <script>
        function findMax() {
            var i;
            var max=0
            for(i=0;i < arguments.length; i++) {
                if (arguments[i] > max) {
                    max = arguments[i];
                }
            }
            return max;
        }
        document.getElementById("result").innerHTML = findMax (1,23,43)
    </script>

